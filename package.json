    "test": "echo \"Warning: no test specified\""
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "prettier": "2.8.3",
    "ts-loader": "9.4.2",
    "typescript": "4.9.5",
    "webpack": "^5.96.1",
    "webpack-cli": "5.0.1"
  },
  "dependencies": {
    "@prismatic-io/translations": "^2.6.1",
    "lodash.merge": "4.6.2",
    "url-join": "5.0.0"
